<div class="author">
  <h1 class="author__heading">Search Result</h1>
</div>
<section>
  <ul class="article-ul">
    <% data.forEach(post => { %>
    <li>
      <a href="/searchResult">
        <span><%= post.title %></span>
        <span class="article-list__date"
          ><%= post.createdAt.toDateString() %></span
        >
      </a>
    </li>
    <% }) %>
  </ul>
</section>
